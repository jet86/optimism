{
  "extends": [
    "tslint-config-standard",
    "tslint:latest"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "space-before-function-paren": false
  }
}

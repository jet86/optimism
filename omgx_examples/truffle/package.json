{
  "name": "@omgx/Truffle-ERC20-Example",
  "private": true,
  "version": "1.0.0-alpha.1",
  "description": "Example of using Optimistic Ethereum compiler & local Optimistic Ethereum nodes with Truffle to run a full ERC20 test suite",
  "scripts": {
    "clean": "rimraf build build-ovm",
    "compile": "truffle compile --config truffle-config",
    "compile:local": "truffle compile --config truffle-config-ovm.js",
    "compile:boba": "truffle compile --config truffle-config-ovm.js",
    "test:integration": "truffle test",
    "test:integration:local": "truffle test --network local_l2 --config truffle-config-ovm.js",
    "test:integration:boba": "truffle test --network boba_rinkeby --config truffle-config-ovm.js",
    "deploy:local": "truffle migrate --network local_l2 --config truffle-config-ovm.js",
    "deploy:boba": "truffle migrate --network boba_rinkeby --config truffle-config-ovm.js"
  },
  "keywords": [
    "optimism",
    "rollup",
    "optimistic",
    "ethereum",
    "truffle",
    "ovm",
    "example",
    "ERC20",
    "token"
  ],
  "homepage": "https://github.com/ethereum-optimism/Truffle-ERC20-Example#readme",
  "license": "MIT",
  "author": "Optimism PBC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereum-optimism/Truffle-ERC20-Example.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@eth-optimism/solc": "0.6.12-alpha.1",
    "@truffle/hdwallet-provider": "^1.4.0",
    "rimraf": "^3.0.2",
    "truffle": "^5.3.6"
  }
}
